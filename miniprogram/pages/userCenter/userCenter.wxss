/* pages/userCenter/userCenter.wxss */
.mainBg {
  background-image: url('https://7365-severless-om62w-1302847609.tcb.qcloud.la/backImage/D_01.png?sign=69c19f389d4bd6d8fb758667b5857f98&t=1597755958');
  background-size: 100% 100%;
  height: 100%;
  position: relative;
  padding-bottom: 350rpx;
  box-sizing: border-box;
}
.top {
  background-image: url('https://7365-severless-om62w-1302847609.tcb.qcloud.la/backImage/%E6%92%AD%E6%94%BE%E8%83%8C%E6%99%AF.png?sign=43a4f849ce90278f6dbe4198f2ac02aa&t=1598100394');
  background-size: 100% 321rpx;
  height: 321rpx;
  position: relative;
  margin-bottom: 70rpx;
}
.top video {
  height: 100%;
  width: 100%;
}
.flex {
  flex-direction: row;
  justify-content: center;
  margin: 77rpx 0;
}
.btn {
  border-radius: 30rpx;
  border: 2rpx solid #133B49;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 172rpx;
  height: 60rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  position: absolute;
  background-color: #fff;
}
.btn.btn_down {
  position: relative;
  background-color: #147483;
  border-color: #147483;
}
.btn.btn_down image {
  width: 26rpx;
}
.btn.btn_down text {
  color: #fff;
  font-size: 24rpx;
}
.btn image {
  width: 12rpx;
}
.btn text {
  font-size: 28rpx;
  color: #133B49;
  margin-left: 12rpx;
}
.my_map {
  width: 100%;
  background-color: #FFD594;
  border-radius: 16rpx;
  padding: 26rpx 56rpx;
  margin-bottom: 74rpx;
}
.my_photo {
  width: 690rpx;
  padding: 26rpx;
  margin: 0 auto;
}
.title {
  margin-bottom: 30rpx;
}
.title image {
  width: 34rpx;
  margin-right: 19rpx;
  vertical-align: baseline;
}
.title text {
  font-size: 30rpx;
  color: #133B49;
}
.photo_list {
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  flex-wrap: wrap;
}
.photo_list .item {
  width: 255rpx;
  border-radius: 16rpx;
  overflow: hidden;
  height: 440rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
}
.photo_list .item image {
  width: 100%;
}
.bottom {
  text-align: center;
  width: 100%;
  position: absolute;
  bottom: 170rpx;
}
.bottom image {
  width: 235rpx;
}
.maptitle {
  color: #133B49;
  font-size: 28rpx;
  display: block;
  text-align: center;
}
.bg {
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
  position: fixed;
  background-color: #000;
}
video {
  top: 50%;
  position: fixed;
  width: 100%;
  z-index: 1000;
  transform: translateY(-50%);
}

.mycanvas{
  position: absolute;
  left: -1000px;
  width: 750px;
  height: 1334px;
}
