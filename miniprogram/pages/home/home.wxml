    <image id="m" class="picture4" src="{{spotImageUrl['法治天下']}}" mode="widthFix" bind:tap="goDetail"/>
    <image id="n" class="picture5" src="{{spotImageUrl['钱端升']}}" mode="widthFix" bind:tap="goDetail"/>
    <image id="y" class="picture6" src="{{spotImageUrl['博物馆']}}" mode="widthFix" bind:tap="goDetail"/>
</view>
<view class="cloud1" hover-class="none" hover-stop-propagation="false"></view>
