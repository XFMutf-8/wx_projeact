.cloud1{
    position: absolute;
    width: 200rpx;
    height: 90rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    top: 3%;
    -webkit-animation-name: an1; 
    -webkit-animation-duration: 100s; /*动画持续时间*/
    -webkit-animation-iteration-count: infinite; /*动画次数*/
    -webkit-animation-delay: 0s; /*延迟时间*/
}
.cloud2{
    position: absolute;
    width: 200rpx;
    height: 60rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    top: 30%;
    -webkit-animation-name: an1; 
    -webkit-animation-duration: 120s; /*动画持续时间*/
    -webkit-animation-iteration-count: infinite; /*动画次数*/
    -webkit-animation-delay: 0s; /*延迟时间*/
}
.cloud3{
    position: absolute;
    width: 200rpx;
    height: 100rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    top: 60%;
    -webkit-animation-name: an2; 
    -webkit-animation-duration: 110s; /*动画持续时间*/
    -webkit-animation-iteration-count: infinite; /*动画次数*/
    -webkit-animation-delay: 0s; /*延迟时间*/
}
@keyframes an1
{
    0%{
        right: -200rpx;
    }
    100%{
        right: 800rpx;
    }
}
@keyframes an2
{
    0%{
        left: -200rpx;
    }
    100%{
        left: 800rpx;
    }
}